{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["__tests__", "dist", "node_modules"],
    "rules": {
        "no-plusplus": "off",
        "no-console": "off",
        "import/no-cycle": "off"
    }
}
